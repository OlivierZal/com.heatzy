{
  "env": {
    "browser": true,
    "es2024": true,
    "node": true
  },
  "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "rules": {
    "import/no-import-module-exports": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["__"]
      }
    ],
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "onHomeyReady"
      }
    ]
  }
}
